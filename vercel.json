{
    "version": 2,
    "builds": [
        {
            "src": "api/main.py",
            "use": "@vercel/python"
        },
        {
            "src": "public/index.html",
            "use": "@vercel/static"
        },
        {
            "src": "public/script.js",
            "use": "@vercel/static"
        },
        {
            "src": "public/styles.css",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/api/main.py"
        },
        {
            "src": "/script.js",
            "dest": "/public/script.js"
        },
        {
            "src": "/styles.css",
            "dest": "/public/styles.css"
        },
        {
            "src": "/(.*)",
            "dest": "/public/index.html"
        }
    ]
}